<!-- 新窗口打开 -->
<base target="_blank">
<!-- 百度验证 -->
<meta name="baidu-site-verification" content="code-8adQav1IKx" />
<!-- 图标动画 -->
<link rel="stylesheet" href="https://cdn.jsdelivr.net/gh/sviptzk/StaticFile_HEXO@latest/butterfly/css/font-awesome-animation.min.css">

<!-- ECharts图表 -->
<script src="https://cdn.jsdelivr.net/npm/echarts@4.9.0/dist/echarts.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/echarts-gl-lrtbx@1.1.2/dist/echarts-gl.min.js"></script>

</head>
